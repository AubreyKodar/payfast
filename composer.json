{
    "name": "aubreykodar/payfast",
    "description": "Payfast api for Laravel",
    "keywords": [
        "aubreykodar",
        "laravel",
        "payfast",
        "payment",
        "payfast api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aubrey Kodar",
            "email": "Hlungwaneak@protonmail.com",
            "homepage": "https://twitter.com/aubreykodar"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.3@dev",
        "monolog/monolog": "1.*",
        "nesbot/carbon": "1.*"
    },
    "require-dev": {
        "phpro/grumphp": "dev-master",
        "squizlabs/php_codesniffer": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "AubreyKodar\\Payfast\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AubreyKodar\\Payfast\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AubreyKodar\\Payfast\\PayfastServiceProvider"
            ]
        }
    }
}
